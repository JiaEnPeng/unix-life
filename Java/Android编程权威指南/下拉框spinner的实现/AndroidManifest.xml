<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="strp.arrival">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"/>
        <activity android:name=".RegisterActivity"/>
        <activity android:name=".GuideMsgActivity"/>
        <activity android:name=".GuideGuideActivity"/>
        <activity android:name=".GuideMapActivity"/>
        <activity android:name=".GuideInfoActivity"/>
        <activity android:name=".GuideRoomActivity"/>
        <activity android:name=".GuideNameActivity"/>
        <activity android:name=".TouristMsgActivity"/>
        <activity android:name=".TouristGuideActivity"/>
        <activity android:name=".TouristMapActivity"/>
        <activity android:name=".TouristInfoActivity"/>
    </application>

</manifest>